{% extends "base.html.twig" %}

{% block title %}
	Director
{% endblock %}

{% block head %}{% endblock %}

{% block content %}

	{% for director in director1 %}
		<div class="card m-2 text-center">
			<div class="card-body">
				<h5 class="card-title">{{director.first_name}} {{director.last_name}}</h5>
				<a href="/Youri/PHP/PhpMvcObjet/directors/{{director.id - 1}}" class="btn m-2 btn-dark">Aller sur la fiche</a>
			</div>
		</div>
	{% endfor %}
	<h2 class="text-success">
		{{onedirector1.first_name}}
		{{onedirector1.last_name}}
	</h2>
{% endblock %}
